<div class="main">
 <h1>This is the Users List Page</h1>
 <button id="btn">Add New User</button>
 <table>
     <tr class="tableHeader">
       <th>First Name</th>
       <th>Last Name</th>
       <th>Email</th>
     </tr>
     {{#each users_arr}}
         <tr class="tableRow" data-id="{{this.id}}">
           <td>{{this.first_name}}</td>
           <td>{{this.last_name}}</td>
           <td>{{this.email}}</td>
         </tr>
     {{/each}}
 </table>
</div>
<script src="/js/users_list.js"></script>
